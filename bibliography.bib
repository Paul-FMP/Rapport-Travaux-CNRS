@article{LazyPPL,
	author = {Dash, Swaraj and Kaddar, Younesse and Paquet, Hugo and Staton, Sam},
	title = {Affine Monads and Lazy Structures for Bayesian Programming},
	year = {2023},
	issue_date = {January 2023},
	publisher = {Association for Computing Machinery},
	address = {New York, NY, USA},
	volume = {7},
	number = {POPL},
	doi = {10.1145/3571239},
	abstract = {We show that streams and lazy data structures are a natural idiom for programming with infinite-dimensional Bayesian methods such as Poisson processes, Gaussian processes, jump processes, Dirichlet processes, and Beta processes. The crucial semantic idea, inspired by developments in synthetic probability theory, is to work with two separate monads: an affine monad of probability, which supports laziness, and a commutative, non-affine monad of measures, which does not. (Affine means that T(1)≅ 1.) We show that the separation is important from a decidability perspective, and that the recent model of quasi-Borel spaces supports these two monads. To perform Bayesian inference with these examples, we introduce new inference methods that are specially adapted to laziness; they are proven correct by reference to the Metropolis-Hastings-Green method. Our theoretical development is implemented as a Haskell library, LazyPPL.},
	journal = {Proc. ACM Program. Lang.},
	month = jan,
	articleno = {46},
	numpages = {31},
	keywords = {synthetic measure theory, quasi-Borel spaces, probabilistic programming, nonparametric statistics, laziness, functional programming, commutative monads, categorical semantics, Haskell, Bayesian inference}
}
 @book{Riehl_Verity_2022, address={Cambridge}, series={Cambridge Studies in Advanced Mathematics}, title={Elements of infinity-Category Theory}, ISBN={978-1-108-83798-9}, url={https://www.cambridge.org/core/books/elements-of-category-theory/DAC48C449AB8C2C1B1E528A49D27FC6D}, DOI={10.1017/9781108936880}, abstractNote={The language of infinity-categories provides an insightful new way of expressing many results in higher-dimensional mathematics but can be challenging for the uninitiated. To explain what exactly an infinity-category is requires various technical models, raising the question of how they might be compared. To overcome this, a model-independent approach is desired, so that theorems proven with any model would apply to them all. This text develops the theory of infinity-categories from first principles in a model-independent fashion using the axiomatic framework of an infinity-cosmos, the universe in which infinity-categories live as objects. An infinity-cosmos is a fertile setting for the formal category theory of infinity-categories, and in this way the foundational proofs in infinity-category theory closely resemble the classical foundations of ordinary category theory. Equipped with exercises and appendices with background material, this first introduction is meant for students and researchers who have a strong foundation in classical 1-category theory.}, publisher={Cambridge University Press}, author={Riehl, Emily and Verity, Dominic}, year={2022}, collection={Cambridge Studies in Advanced Mathematics} }

