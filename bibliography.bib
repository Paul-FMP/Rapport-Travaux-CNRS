 @article{Liell-Cock_Staton_2025, title={Compositional Imprecise Probability: A Solution from Graded Monads and Markov Categories}, volume={9}, DOI={10.1145/3704890}, abstractNote={Imprecise probability is concerned with uncertainty about which probability distributions to use. It has applications in robust statistics and machine learning.        We look at programming language models for imprecise probability. Our desiderata are that we would like our model to support all kinds of composition, categorical and monoidal; in other words, guided by dataflow diagrams. Another equivalent perspective is that we would like a model of synthetic probability in the sense of Markov categories.        Imprecise probability can be modelled in various ways, with the leading monad-based approach using convex sets of probability distributions. This model is not fully compositional because the monad involved is not commutative, meaning it does not have a proper monoidal structure. In this work, we provide a new fully compositional account. The key idea is to name the non-deterministic choices. To manage the renamings and disjointness of names, we use graded monads. We show that the resulting compositional model is maximal and relate it with the earlier monadic approach, proving that we obtain tighter bounds on the uncertainty.}, number={POPL}, journal={Proc. ACM Program. Lang.}, author={Liell-Cock, Jack and Staton, Sam}, year={2025}, month=jan, pages={54:1596-54:1626} }

